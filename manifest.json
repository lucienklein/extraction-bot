{
  "manifest_version": 3,
  "name": "Querco - Extraction",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": ["*://172.30.69.50/*"],
      "js": ["content.js", "utils/extractFile.js"],
      "css": ["style.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "popup.html",
  "permissions": ["storage"],
  "web_accessible_resources": ["dwt/*", "utils/*"]
}
