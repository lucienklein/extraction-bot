{
  "manifest_version": 2,
  "name": "Querco - Extraction",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": ["*://172.30.69.50/*"],
      "js": ["dist/bundle.js"],
      "css": ["style.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "popup.html",
  "permissions": ["storage"],
  "web_accessible_resources": ["dwt/*"]
}
