{
  "manifest_version": 2,
  "name": "Querco - Extraction",
  "version": "1.3",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "include_globs": ["*://*/moduleSil/demande/saisie/index.php*"],
      "js": ["bundle.js"],
      "css": ["style.css"]
    }
  ],
  "options_page": "options.html",
  "permissions": ["storage"],
  "web_accessible_resources": ["dwt/*", "images/*"]
}
