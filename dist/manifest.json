{
  "manifest_version": 2,
  "name": "Querco - Extraction",
  "version": "1.3",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://172.30.69.50/*"],
      "js": ["bundle.js"],
      "css": ["style.css"]
    }
  ],
  "options_page": "options.html",
  "permissions": ["storage", "downloads"],
  "web_accessible_resources": ["dwt/*", "images/*"]
}
